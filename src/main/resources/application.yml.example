spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/vectoros
    username: postgres
    password: postgres
    driver-class-name: org.postgresql.Driver

  jpa:
    hibernate:
      ddl-auto: validate
    properties:
      hibernate:
        format_sql: true
    open-in-view: false
    show-sql: true

  flyway:
    enabled: true
    locations:
      - classpath:db/migration

  telegram:
    bot:
      token: "YOUR_BOT_TOKEN_HERE"  # Получите у @BotFather в Telegram
      webapp:
        url: "https://yourdomain.com/cabinet"  # URL вашего веб-приложения кабинета

server:
  port: 8080


